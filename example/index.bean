option "title" "我的账本"
option "operating_currency" "CNY"
option "render_commas" "TRUE"

; fava 配置
1970-01-01 custom "fava-option" "interval" "day"
1970-01-01 custom "fava-option" "language" "zh_CN"

; 账户定义
include "./account/assets.bean"
include "./account/equity.bean"
include "./account/expenses.bean"
include "./account/income.bean"
include "./account/liabilities.bean"
; 数据
include "./month/months.bean"
; 多币种配置
include "./price/prices.bean"
; 历史数据
include "./history.bean"

